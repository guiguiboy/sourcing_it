<h1>Job</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Created_at</th>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Company_id</th>
            <td>{{ entity.companyid }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Source</th>
            <td>{{ entity.source }}</td>
        </tr>
        <tr>
            <th>Contact_name</th>
            <td>{{ entity.contactname }}</td>
        </tr>
        <tr>
            <th>Contact_email</th>
            <td>{{ entity.contactemail }}</td>
        </tr>
        <tr>
            <th>Contact_phone</th>
            <td>{{ entity.contactphone }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('SourcingItSiteBundle_job') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('SourcingItSiteBundle_job_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('SourcingItSiteBundle_job_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
